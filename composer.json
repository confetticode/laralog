{
    "name": "confetticode/telegram",
    "description": "Telegram integration",
    "authors": [
        {
            "name": "Quynh Nguyen",
            "email": "seriquynh@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "monolog/monolog": "^2.3",
        "illuminate/log": "^8.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.3",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ConfettiCode\\Laralog\\": "./src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ConfettiCode\\Laralog\\TelegramServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
